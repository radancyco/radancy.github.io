{
 "manifest_version": 2,
 "name": "Accessibility Checklist",
 "version": "1.0.0",
 "description": "This is a sample description",
 "permissions": [
   "activeTab",
   "declarativeContent"
 ],
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["init.js"]
   }
 ],
 "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://radancy.dev; object-src 'self'",
 "browser_action": {
   "default_title": "Does a thing when you do a thing",
   "default_popup": "init.html"
 }
}
